{
    "require": {
        "php": "^8.1",
        "symfony/var-dumper": "*", // дампы
        "illuminate/collections": "*",
        "vlucas/phpdotenv": "*", // env file
        "bacon/bacon-qr-code": "*", // qr
        "dasprid/enum": "*", // qr
        "hflabs/dadata": "*", // dadata
        "andreyryabin/sprint.migration": "dev-master",
        "smsaero/smsaero_api": "^1.0",
        "wikimedia/composer-merge-plugin": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "": "local/lib/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "merge-plugin": {
            "require": [
                "bitrix/composer-bx.json"
            ]
        }
    }
}