{
  "extra": {
    "bitrix-dir": "bitrix",
    "installer-paths": {
      "local/modules/{$name}/": [
        "type:bitrix-module"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://git.itb-dev.ru/ITB-dev/itb.seo.git"
    }
  ],
  "require": {
    "php": "^8.2",
    "symfony/var-dumper": "*",
    "illuminate/collections": "*",
    "vlucas/phpdotenv": "*",
    "bacon/bacon-qr-code": "*",
    "dasprid/enum": "*",
    "hflabs/dadata": "*",
    "andreyryabin/sprint.migration": "dev-master",
    "smsaero/smsaero_api": "^1.0",
    "wikimedia/composer-merge-plugin": "dev-master",
    "itb/itb.seo": "^1.0",
    "league/oauth2-server": "^9.2"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "wikimedia/composer-merge-plugin": true
    }
  },
  "autoload": {
    "psr-4": {
      "": "local/lib/"
    }
  }
}
