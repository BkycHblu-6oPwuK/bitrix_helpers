{
  "extra": {
    "bitrix-dir": "bitrix",
    "installer-paths": {
      "local/modules/{$name}/": [
        "type:bitrix-module"
      ]
    },
    "merge-plugin": {
      "require": [
        "bitrix/composer-bx.json"
      ]
    }
  },
  "require": {
    "php": "^8.2",
    "symfony/var-dumper": "*",
    "illuminate/collections": "*",
    "vlucas/phpdotenv": "*",
    "bacon/bacon-qr-code": "*",
    "dasprid/enum": "*",
    "hflabs/dadata": "*",
    "andreyryabin/sprint.migration": "dev-master",
    "smsaero/smsaero_api": "^1.0",
    "wikimedia/composer-merge-plugin": "dev-master",
    "league/oauth2-server": "^9.2",
    "psr/log": "^3.0",
    "firebase/php-jwt": "^6.11",
    "symfony/uid": "^7.3"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "wikimedia/composer-merge-plugin": true
    }
  },
  "autoload": {
    "psr-4": {
      "": "local/lib/"
    }
  }
}